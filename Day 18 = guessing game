print("One-Million-To_One")
print()
rightanswer = 2626
while True:
  guess = float(input("What is your guess? "))
  if guess < 2626:
    print("Too low")
  elif guess > 2626:
    print("Too high")
  elif guess == 2626:
    print("Hurray, you guessed the number!")
    exit()
  else:
    continue
    
